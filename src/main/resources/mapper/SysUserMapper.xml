<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yzjk.system.sys.mapper.UserMapper">

	<resultMap id="sysUserMap" type="com.yzjk.system.sys.entity.SysUser">
         <id property="userId" column="user_id"/>
	    <result property="userName" column="user_name"/>
        <result property="password" column="password"/>
        <result property="name" column="name"/>
        <result property="idcard" column="idcard"/>
        <result property="mobilePhone" column="mobile_phone"/>
        <result property="email" column="email"/>
        <result property="userStatus" column="user_status"/>
        <result property="lastLoginStudioId" column="last_login_studio_id"/>
        <result property="lastLoginOrgId" column="last_login_org_id"/>
        <result property="createTime" column="create_time"/>
    </resultMap>

	<sql id="Base_Column_List">
		user_id,user_name,password,name,idcard,mobile_phone,email,user_status,last_login_studio_id,last_login_org_id,create_time
	</sql>

</mapper>
